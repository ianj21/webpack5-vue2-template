{
  "name": "webpack5-vue2-template",
  "version": "1.0.0",
  "author": "P2K0",
  "description": "A template for webpack5 vue2 project",
  "keywords": [
    "webpack5",
    "vue2",
    "template"
  ],
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server -c ./build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack -c ./build/webpack.config.prod.js",
    "server:speed": "webpack-dev-server -c ./build/webpack.config.speed.js",
    "build:analyzer": "webpack -c ./build/webpack.config.analyzer.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "vue": "^2.6.14",
    "vue-router": "3",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.9",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-loader": "^8.2.4",
    "core-js": "^3.22.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
